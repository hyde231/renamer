{
  "name": "porn-renamer",
  "description": "A cli to rename your porn files using http://metadataapi.net",
  "version": "0.0.1",
  "author": "ChalupaBatMan",
  "bugs": "https://github.com/theporndb/renamer/issues",
  "dependencies": {
    "axios": "^0.19.0",
    "cli-ux": "^5.4.1",
    "commander": "^4.0.1",
    "inquirer": "^7.0.1",
    "pkg": "^4.4.2",
    "string-template": "^1.0.0",
    "tslib": "^1",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "@types/node": "^10",
    "eslint": "^5.13",
    "rimraf": "^3.0.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/theporndb/renamer",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "theporndb/renamer",
  "scripts": {
    "prepack": "rimraf lib && tsc -b",
    "build": "npm run prepack && npm run pkg",
    "pkg": "pkg . --out-path=./build -t node10-linux-x64,node10-macos-x64,node10-win-x64"
  },
  "bin": "./lib/index.js",
  "types": "lib/index.d.ts",
  "pkg": {
    "scripts": "./lib/*.js",
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  }
}
